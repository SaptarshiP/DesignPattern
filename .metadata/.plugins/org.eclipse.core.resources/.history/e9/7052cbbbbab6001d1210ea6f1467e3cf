import com.psja.model.BROWSER_NAME;

public class FacadeDesign {

	public void facadeWork( BROWSER_NAME browserName ) {
		
		switch( browserName ) {
			case CHROME:
				{
					Driver driver = new ChromeDriver();
					System.out.println( driver.getDriver() );
					break;
				}
			case OPERA:
				{
					break;
				}
			case EXPLORER:
				{
					break;
				}
		}
		
	}
	
}
